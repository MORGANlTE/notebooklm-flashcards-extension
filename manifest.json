{
  "manifest_version": 3,
  "name": "NotebookLM Flashcard Generator",
  "version": "3.2",
  "description": "Adds a button to NotebookLM to generate and download flashcards from all sources.",
  "author": "Morganite",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://notebooklm.google.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
